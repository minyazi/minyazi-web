<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="test.mybatis">
	<select id="getProcessMesg" parameterType="String" resultType="com.minyaziweb.domain.ProcessInfo">
		select processMesg from ProcessInfo where processCode=#{processCode}
	</select>
	<select id="getProcessMesgs" resultType="com.minyaziweb.domain.ProcessInfo">
		select processCode,processMesg from ProcessInfo order by processCode
	</select>
</mapper>
